<?xml version="1.0" encoding="UTF-8"?>
<company:CompanyModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:company="http://cs.gssi.it/company">
  <company name="IBM"
      address="//@adress">
    <persons
        firstname="John"
        lastname="Doe"
        assignedTo="//@company/@projects.0"
        employed="//@company/@lines.1"/>
    <projects
        xsi:type="company:European"
        name="BIRDS"
        related="//@categories.0"
        budget="1000"/>
    <lines xsi:type="company:Unit"
        name="SE"/>
    <lines xsi:type="company:ServiceLine"
        name="OS-dev"/>
  </company>
  <categories
      name="Software Engineering"
      topics="//@topics.0"/>
  <topics id="Software Development"
      category="//@categories.0"/>
  <adress city="Rome"
      completeAddress="v.le crispi"/>
</company:CompanyModel>
